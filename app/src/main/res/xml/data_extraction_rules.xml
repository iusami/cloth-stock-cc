<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- クラウドバックアップから除外するデータを指定 -->
        <exclude domain="database" />
        <exclude domain="file" path="temp/" />
    </cloud-backup>
    <device-transfer>
        <!-- デバイス間転送から除外するデータを指定 -->
        <!-- databaseドメインではサブディレクトリパスは使用できないため、特定のDB名で除外 -->
        <exclude domain="database" path="cache.db" />
        <exclude domain="database" path="cache.db-shm" />
        <exclude domain="database" path="cache.db-wal" />
    </device-transfer>
</data-extraction-rules>